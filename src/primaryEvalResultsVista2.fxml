<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<StackPane fx:id="primaryEvalResultsStackPane" minHeight="1080.0" minWidth="1920.0" stylesheets="@vista.css" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="PrimaryEvalResultsController">
   <children>
      <VBox fx:id="primaryEvalVBox" alignment="CENTER" prefHeight="906.0" prefWidth="1457.0" StackPane.alignment="CENTER">
         <children>
            <Label fx:id="primaryEvalResultsLabel" alignment="CENTER" text="PERFORMANCE SUMMARY" textFill="WHITE">
               <font>
                  <Font name="Microsoft JhengHei UI Bold" size="35.0" />
               </font>
            </Label>
            <Text fx:id="primaryEvalResultsText" fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="Guidelines recommend at least 80% of compressions at a rate of 100-120, a depth of 5-6cm, and allowing for full chest recoil." textAlignment="CENTER" wrappingWidth="600.0">
               <font>
                  <Font name="Microsoft JhengHei UI" size="30.0" />
               </font>
            </Text>
            <Label fx:id="primaryEvalResultsLabelTwo" text="Recoil:" textFill="WHITE">
               <font>
                  <Font name="Microsoft JhengHei UI" size="30.0" />
               </font>
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
               <graphic>
                  <TextField fx:id="recoilTextField" alignment="CENTER_RIGHT" />
               </graphic>
            </Label>
            <Rectangle fx:id="primaryEvalResultsRecOne" arcHeight="5.0" arcWidth="5.0" fill="WHITE" height="100.0" stroke="WHITE" strokeType="INSIDE" width="400.0" />
            <Label fx:id="primaryEvalResultsLabelThree" text="Depth:" textFill="WHITE">
               <font>
                  <Font name="Microsoft JhengHei UI" size="30.0" />
               </font>
            </Label>
            <Rectangle fx:id="primaryEvalResultsRecTwo" arcHeight="5.0" arcWidth="5.0" fill="WHITE" height="100.0" stroke="WHITE" strokeType="INSIDE" width="400.0" />
            <Label fx:id="primaryEvalResultsLabelFour" text="Rate:" textFill="WHITE">
               <font>
                  <Font name="Microsoft JhengHei UI" size="30.0" />
               </font>
            </Label>
            <Rectangle fx:id="primaryEvalResultsRecThree" arcHeight="5.0" arcWidth="5.0" fill="WHITE" height="100.0" stroke="WHITE" strokeType="INSIDE" width="400.0" />
         </children>
      </VBox>
   </children>
</StackPane>
