<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<StackPane fx:id="evalOneResultsVista" minHeight="800.0" minWidth="1200.0" stylesheets="@vista.css" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="evalOneResultsController">
   <children>
      <Button fx:id="evalOneResultsNextButton" mnemonicParsing="false" onAction="#evalOneNextAction" text="&gt;" textFill="WHITE" StackPane.alignment="BOTTOM_CENTER">
         <font>
            <Font name="Microsoft JhengHei UI" size="130.0" />
         </font>
      </Button>
      <VBox fx:id="evalOneResultsVBox" alignment="CENTER" maxWidth="1000.0">
         <children>
            <Label fx:id="evalOneResultsLabel" alignment="CENTER" text="Your CPR Performance Results:" textAlignment="CENTER" textFill="WHITE">
               <font>
                  <Font name="Microsoft JhengHei UI Bold" size="35.0" />
               </font>
               <VBox.margin>
                  <Insets top="-100.0" />
               </VBox.margin>
            </Label>
            <Text fx:id="evalOneResultsText" fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="Guidelines recommend at least 80% of compressions at a rate of 100-120, a depth of 5-6cm, and allowing for full chest recoil." textAlignment="CENTER" wrappingWidth="800.0">
               <font>
                  <Font name="Microsoft JhengHei UI" size="25.0" />
               </font>
               <VBox.margin>
                  <Insets top="50.0" />
               </VBox.margin>
            </Text>
            <HBox maxWidth="800.0" minWidth="810.0" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <VBox minHeight="300.0" minWidth="270.0" prefWidth="270.0">
                     <children>
                        <Label alignment="TOP_CENTER" contentDisplay="CENTER" text="Rate" textAlignment="CENTER" textFill="WHITE">
                           <font>
                              <Font name="Microsoft JhengHei UI" size="35.0" />
                           </font>
                           <VBox.margin>
                              <Insets left="125.0" />
                           </VBox.margin>
                        </Label>
                        <StackPane minHeight="200.0" minWidth="270.0" prefHeight="300.0" prefWidth="270.0">
                           <children>
                              <TextField fx:id="evalOneResultsRate" accessibleRole="TEXT" alignment="CENTER" maxWidth="250.0" minHeight="150.0" promptText="Rate">
                                 <font>
                                    <Font name="Microsoft JhengHei UI" size="40.0" />
                                 </font>
                                 <StackPane.margin>
                                    <Insets />
                                 </StackPane.margin>
                              </TextField>
                           </children>
                        </StackPane>
                     </children>
                  </VBox>
                  <VBox minWidth="270.0" prefHeight="200.0" prefWidth="270.0">
                     <children>
                        <Label text="Depth" textFill="WHITE">
                           <font>
                              <Font size="35.0" />
                           </font>
                           <VBox.margin>
                              <Insets left="100.0" />
                           </VBox.margin>
                        </Label>
                        <TextField fx:id="evalOneResultsDepth" accessibleRole="TEXT" alignment="CENTER" maxWidth="250.0" minHeight="150.0" minWidth="250.0" promptText="Depth">
                           <font>
                              <Font name="Microsoft JhengHei UI" size="40.0" />
                           </font>
                           <VBox.margin>
                              <Insets left="20.0" top="50.0" />
                           </VBox.margin>
                        </TextField>
                     </children>
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </VBox>
                  <VBox minWidth="270.0" prefHeight="300.0" prefWidth="270.0">
                     <children>
                        <Label text="Recoil" textFill="WHITE">
                           <font>
                              <Font size="35.0" />
                           </font>
                           <VBox.margin>
                              <Insets left="100.0" />
                           </VBox.margin>
                        </Label>
                        <TextField fx:id="evalOneResultsRecoil" alignment="CENTER" maxWidth="250.0" minHeight="150.0" minWidth="250.0" promptText="Recoil">
                           <font>
                              <Font name="Microsoft JhengHei UI" size="40.0" />
                           </font>
                           <VBox.margin>
                              <Insets left="25.0" top="50.0" />
                           </VBox.margin>
                        </TextField>
                     </children>
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </VBox>
               </children>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <VBox.margin>
                  <Insets top="50.0" />
               </VBox.margin>
            </HBox>
         </children>
         <stylesheets>
            <URL value="@vista.css" />
            <URL value="@vista.css" />
         </stylesheets>
      </VBox>
   </children>
</StackPane>
