<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<StackPane fx:id="stepSixStackPane" prefHeight="1080.0" prefWidth="1920.0" stylesheets="@vista.css" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="stepSixVistaController">
   <children>
      <VBox fx:id="stepSixVBox" alignment="CENTER" maxWidth="800.0" stylesheets="@vista.css">
         <children>
            <Label fx:id="stepSixLabel" alignment="CENTER" text="Step 3: Re-evalutation" textAlignment="CENTER" textFill="WHITE">
               <font>
                  <Font name="Microsoft JhengHei UI Bold" size="35.0" />
               </font>
            </Label>
            <Text fx:id="stepSixText" fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="You will perform 1 minute of chest compressions without visual feedback again" textAlignment="CENTER" wrappingWidth="800.0">
               <font>
                  <Font name="Microsoft JhengHei UI" size="30.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="30.0" top="70.0" />
               </VBox.margin>
            </Text>
            <Button fx:id="stepSixNextButton" alignment="CENTER" contentDisplay="CENTER" maxWidth="300.0" minHeight="100.0" mnemonicParsing="false" onAction="#stepSixNextAction" text="GET STARTED">
               <font>
                  <Font name="Microsoft JhengHei UI Bold" size="35.0" />
               </font>
               <VBox.margin>
                  <Insets top="50.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </children>
</StackPane>
