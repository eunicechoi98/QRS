<?xml version="1.0" encoding="UTF-8"?>

<?scenebuilder-stylesheet vista.css?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.StackPane?>

<StackPane fx:id="breakvista" prefHeight="1080.0" prefWidth="1920.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="BreakVistaController">
  <children>
      <VBox alignment="TOP_CENTER">
         <children>
            <Label alignment="CENTER" text="Break time. You now have 1 minute to relax before your post-training assessment." textAlignment="CENTER" textFill="#000000cb" wrapText="true">
               <font>
                  <Font name="System Bold" size="30.0" />
               </font>
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </Label>
            <Label text="Please do not perform any compressions during your break. Please do not leave the room." wrapText="true">
               <VBox.margin>
                  <Insets top="5.0" />
               </VBox.margin>
               <font>
                  <Font size="28.0" />
               </font>
            </Label>
            <Label layoutX="493.0" layoutY="148.0" text="Be ready to start compressions as soon as the break time is finished." wrapText="true">
               <VBox.margin>
                  <Insets top="5.0" />
               </VBox.margin>
               <font>
                  <Font size="28.0" />
               </font>
            </Label>
            <Label layoutX="340.0" layoutY="85.0" text="Your break ends in:" textAlignment="CENTER" textFill="#000000cb">
               <font>
                  <Font size="48.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="50.0" top="10.0" />
               </VBox.margin>
            </Label>
         </children>
      </VBox>
      <Label fx:id="timerLabel4" textAlignment="CENTER">
         <font>
            <Font name="System Bold" size="250.0" />
         </font>
         <StackPane.margin>
            <Insets />
         </StackPane.margin>
      </Label>
  </children>
</StackPane>
