<?xml version="1.0" encoding="UTF-8"?>

<?scenebuilder-stylesheet vista.css?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<StackPane fx:id="breakvista" prefHeight="800.0" prefWidth="1200.0" stylesheets="@vista.css" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="BreakVistaController">
  <children>
      <Label fx:id="timerLabel4" textAlignment="CENTER">
         <font>
            <Font name="System Bold" size="200.0" />
         </font>
         <StackPane.margin>
            <Insets top="200.0" />
         </StackPane.margin>
      </Label>
      <VBox alignment="TOP_CENTER" stylesheets="@vista.css">
         <children>
            <Label alignment="CENTER" minWidth="800.0" prefWidth="800.0" text="Break time. You now have 1 minute to relax before your post-training assessment." textAlignment="CENTER" textFill="#ffffffcc" wrapText="true">
               <font>
                  <Font name="Microsoft JhengHei UI Bold" size="30.0" />
               </font>
               <VBox.margin>
                  <Insets top="100.0" />
               </VBox.margin>
            </Label>
            <Label prefWidth="700.0" stylesheets="@vista.css" text="Please do not perform any compressions during your break. Please do not leave the room. Be ready to start compressions as soon as the break time is finished." textAlignment="CENTER" textFill="WHITE" wrapText="true">
               <VBox.margin>
                  <Insets top="5.0" />
               </VBox.margin>
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <Label layoutX="493.0" layoutY="148.0" prefWidth="800.0" textAlignment="CENTER" textFill="WHITE" wrapText="true">
               <VBox.margin>
                  <Insets top="5.0" />
               </VBox.margin>
               <font>
                  <Font size="28.0" />
               </font>
            </Label>
         </children>
      </VBox>
  </children>
</StackPane>
